<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_launcher_background"
    tools:context=".MainActivity">


    <TableLayout
        android:id="@+id/tableLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView1"
        android:layout_marginTop="10dp"
        android:shrinkColumns="*"
        android:stretchColumns="*" >

        <TableRow
            android:id="@+id/tableRowBtn"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:gravity="center_horizontal"
            android:paddingBottom="10dp"
            >
            <TextView
                android:id="@+id/txa"
                android:text=""
                android:padding="2dp">
            </TextView>
            <Button
                android:id="@+id/btn_constrain"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableTop="@drawable/ic_launcher_foreground"
                android:text="constrain"
                android:textStyle="bold"
                android:background="@drawable/ic_launcher_background"
                android:gravity="center|center_horizontal"
                android:padding="1dp"
                android:onClick="btnconstrain"
                />
            <TextView
                android:id="@+id/txb"
                android:text=""
                android:padding="2dp">
            </TextView>
            <Button
                android:id="@+id/btnlinier"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableTop="@drawable/ic_launcher_foreground"
                android:text="linier"
                android:textStyle="bold"
                android:background="@drawable/ic_launcher_background"
                android:gravity="center|center_horizontal"
                android:padding="1dp"
                android:onClick="btnlinier"/>
            <TextView
                android:id="@+id/txc"
                android:text=""
                android:padding="2dp">
            </TextView>
            <Button
                android:id="@+id/btnframe"
                android:visibility="visible"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableTop="@drawable/ic_launcher_foreground"
                android:text="Frame"
                android:textStyle="bold"
                android:background="@drawable/ic_launcher_background"
                android:gravity="center|center_horizontal"
                android:padding="1dp"
                android:onClick="btnframe"
                />
            <TextView
                android:id="@+id/txd"
                android:text=""
                android:padding="2dp">
            </TextView>
        </TableRow>

        <TableRow
            android:id="@+id/tableRowBtn1"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:gravity="center_horizontal"
            android:paddingBottom="10dp"
            >
            <TextView
                android:id="@+id/txa1"
                android:text=""
                android:padding="2dp">
            </TextView>
            <Button
                android:id="@+id/btnhorizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableTop="@drawable/ic_launcher_foreground"
                android:text="Horizontal"
                android:textStyle="bold"
                android:background="@drawable/ic_launcher_background"
                android:gravity="center|center_horizontal"
                android:padding="1dp"
                android:onClick="btnhorizontal"
                />
            <TextView
                android:id="@+id/txb1"
                android:text=""
                android:padding="2dp">
            </TextView>
            <Button
                android:id="@+id/btnnaterial"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableTop="@drawable/ic_launcher_foreground"
                android:text="Material"
                android:textStyle="bold"
                android:background="@drawable/ic_launcher_background"
                android:gravity="center|center_horizontal"
                android:padding="1dp"
                android:onClick="btnmaterial"/>
            <TextView
                android:id="@+id/txc2"
                android:text=""
                android:padding="2dp">
            </TextView>
            <Button
                android:id="@+id/btnrelative"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableTop="@drawable/ic_launcher_foreground"
                android:text="relative"
                android:textStyle="bold"
                android:background="@drawable/ic_launcher_background"
                android:gravity="center|center_horizontal"
                android:padding="1dp"
                android:onClick="btnrelative"
                />
            <TextView
                android:id="@+id/txd3"
                android:text=""
                android:padding="2dp">
            </TextView>
        </TableRow>

        <TableRow
            android:id="@+id/tableRowBtn2"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:gravity="center_horizontal">

            <TextView
                android:id="@+id/txi"
                android:text=""
                android:padding="2dp">
            </TextView>
            <Button
                android:id="@+id/btnscroll"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableTop="@drawable/ic_launcher_foreground"
                android:text="scroll"
                android:textStyle="bold"
                android:background="@drawable/ic_launcher_background"
                android:gravity="center|center_horizontal"
                android:onClick="btnscroll"/>
            <TextView
                android:id="@+id/txe"
                android:text=""
                android:padding="2dp">
            </TextView>
            <Button
                android:id="@+id/btntabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableTop="@drawable/ic_launcher_foreground"
                android:text="tabel"
                android:textStyle="bold"
                android:background="@drawable/ic_launcher_background"
                android:gravity="center|center_horizontal"
                android:onClick="btntabel"/>
            <TextView
                android:id="@+id/txf"
                android:text=""
                android:padding="2dp">
            </TextView>

        </TableRow>

    </TableLayout>

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:gravity="center_horizontal"
        android:src="@drawable/ic_launcher_foreground" />


</RelativeLayout>